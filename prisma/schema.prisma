
generator client {
  provider = "prisma-client-js"
  //output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          Int      @id @default(autoincrement())
  login       String   @unique
  email       String   @unique
  password    String
  age         Int      
  description String?  @db.VarChar(1000)
  createdAt   DateTime @default(now())
  updateAt    DateTime @updatedAt
}
